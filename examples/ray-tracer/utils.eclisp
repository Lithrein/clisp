(%:include ("../../include/macros.eclisph"))

(%:include <stdlib.h> <time.h>)

(defun random_double (-> double void)
  (return (/ (rand) (+ 1.0 RAND_MAX))))

(defun random_double_mm (-> double (min max double))
  (return (+ min (* (- max min) (random_double)))))

(defun max_i (-> int (a b int))
  (return (?: (> a b) a b)))

(defun degrees_to_radians (-> double (degrees double))
  (return (/ (* degrees 3.14) 180.0)))
